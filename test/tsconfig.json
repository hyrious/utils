{
  "compilerOptions": {
    "jsx": "react-jsx",
    "noEmit": true,
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "useUnknownInCatchVariables": false,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "types": ["node", "vitest"],
    "moduleResolution": "node"
  }
}
